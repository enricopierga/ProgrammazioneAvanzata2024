				{
					"name": "loginbyOperatore",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"const response = pm.response.json();\r",
									"pm.globals.set(\"BEARER_TOKEN\", response.accessToken);\r",
									"\r",
									"let CryptoJs = require('crypto-js');\r",
									"\r",
									"a = response.accessToken.split('.');\r",
									"var words = CryptoJs.enc.Base64.parse(a[1]);\r",
									"var textString = CryptoJs.enc.Utf8.stringify(words);\r",
									"\r",
									"const payload = JSON.parse(textString);\r",
									"pm.globals.set(\"USER_ID\", payload.userId);"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
